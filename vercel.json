{
    "builds": [
        {
            "src": "src/**/*.scss",
            "use": "@vercel/static-build"
        },
        {
            "src": "package.json",
            "use": "@vercel/static-build"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}